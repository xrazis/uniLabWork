<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import java.lang.String?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Client_Controller_Options01">
    <Label layoutX="175.0" layoutY="58.0" text="Options Menu">
        <font>
            <Font size="36.0"/>
        </font>
    </Label>
    <Group layoutX="221.0" layoutY="148.0">
        <Label layoutX="-5.0" text="Choose connection speed"/>
        <ChoiceBox layoutX="4.0" layoutY="25.0" prefWidth="150.0" fx:id="speedChoiceBox">
            <String fx:value="0.2Mbps"/>
            <String fx:value="0.5Mbps"/>
            <String fx:value="1Mbps"/>
            <String fx:value="3Mbps"/>
        </ChoiceBox>
    </Group>
    <Group layoutX="225.0" layoutY="225.0">
        <Label layoutX="15.0" text="Choose file format"/>
        <ChoiceBox layoutY="25.0" prefWidth="150.0" fx:id="formatChoiceBox">
            <String fx:value=".avi"/>
            <String fx:value=".mp4"/>
            <String fx:value=".mkv"/>
        </ChoiceBox>
    </Group>
    <Button layoutX="244.0" layoutY="317.0" mnemonicParsing="false" style="-fx-background-color: teal;" text="Submit"
            textFill="WHITE" onAction="#getSpeedFormat">
        <font>
            <Font size="24.0"/>
        </font>
    </Button>
</AnchorPane>
